import java.util.*;
/**
 * @author Javier Octavio
 */
public class Principal {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here
        Scanner input = new Scanner(System.in);
        System.out.println("La Coordenada en x");
        int iX;
        
        iX = input.nextInt();
        System.out.println("La Coordenada en y");
        int iY;
        
        iY = input.nextInt();
        if (iX > 0 && iY > 0) {
            System.out.println("Pertenece al Cuadrante I");
        } else {
            if (iX < 0 && iY > 0) {
                System.out.println("Pertenece al Cuadrante II");
            } else {
                if (iX < 0 && iY < 0) {
                    System.out.println("Pertenece al Cuadrante III");
                } else {
                    if (iX > 0 && iY < 0) {
                        System.out.println("Pertenece al Cuadrante IV");
                    }
                }
            }
        }
    }
}
