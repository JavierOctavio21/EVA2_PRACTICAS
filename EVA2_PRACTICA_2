    public static void main(String[] args) {
        // TODO code application logic here
        Scanner input = new Scanner(System.in);
        System.out.println("Ingresa un NÃºmero");
        int iNum;
        
        iNum = input.nextInt();
        int iRes;
        
        iRes = iNum % 2;
        if (iRes == 0) {
            System.out.println("Es PAR");
        } else {
            System.out.println("Es IMPAR");
        }
    }
